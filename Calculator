import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class ChapterOne {
    static public void main(String[] args) throws IOException {
        BufferedReader num1 = new BufferedReader(new InputStreamReader(System.in));
        BufferedReader num2 = new BufferedReader(new InputStreamReader(System.in));
        int Nr1 = Integer.valueOf(num1.readLine());
        int Nr2 = Integer.valueOf(num2.readLine());
        int i;
        int sum =0;
        for (i=Nr1;i<Nr2;i++)
        {
            sum +=i;
        }
        System.out.println("Suma de la " + Nr1 + " la " + i + " este " + sum);
    }
}
